@model IEnumerable<EsyaStore.Identity.ApplicationUser>
@{
    ViewBag.Title = "Table";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<div class="col-md-offset-2">
    <div class="content-panel">
        <hr>
        <table class="table">


            <thead>
                <tr>

                    <th>Name</th>
                    <th>Surname</th>
                    <th>Phone</th>
                    <th>Email</th>
                    <th>UserName</th>
                </tr>
            </thead>
            @foreach (var item in Model)
            {
                <tbody>
                    <tr>

                        <td>@item.Name</td>
                        <td>@item.Surname</td>
                        <td>@item.Phone</td>
                        <td>@item.Email</td>
                        <td>@item.UserName</td>
                    </tr>

                </tbody>
            }
        </table>
    </div><! --/content-panel -->
</div><!-- /col-md-12 -->
